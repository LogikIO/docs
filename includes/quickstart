    <span class="na">name</span><span class="p">:</span> <span class="s1">'Volare'</span><span class="p">,</span>
    <span class="na">description</span><span class="p">:</span> <span class="s1">'a beautiful italian dinner'</span><span class="p">,</span>
    <span class="na">meal</span><span class="p">:</span> <span class="p">{</span>
      <span class="na">course_1</span><span class="p">:</span> <span class="s1">'pasta'</span><span class="p">,</span>
      <span class="na">course_2</span><span class="p">:</span> <span class="s1">'pizza'</span>
    <span class="p">}</span>
  <span class="p">}</span>

  <span class="nx">Stamplay</span><span class="p">.</span><span class="nb">Object</span><span class="p">(</span><span class="s1">'dinner'</span><span class="p">)</span>
    <span class="p">.</span><span class="nx">save</span><span class="p">(</span><span class="nx">dinner</span><span class="p">)</span>
      <span class="p">.</span><span class="nx">then</span><span class="p">(</span><span class="kd">function</span><span class="p">(</span><span class="nx">res</span><span class="p">)</span> <span class="p">{</span>
        <span class="c1">// success</span>
      <span class="p">},</span> <span class="kd">function</span><span class="p">(</span><span class="nx">err</span><span class="p">)</span> <span class="p">{</span>
